{
	"name": "phaser3-parcel-template",
	"version": "1.0.0",
	"description": "A template project for Phaser 3 using Parceljs",
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist",
		"start": "parcel src/index.html -p 8000",
